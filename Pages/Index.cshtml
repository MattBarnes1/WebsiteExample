@page
@model WebApplication5.Pages.NewIndexModel
@{
    ViewData["Title"] = "The Den Development Studios";
    ViewData["Background"] = "{ background-image: url(\"~/images/City-no-bar-grunge.jpg\"); background - position: top; background-attachment: fixed; }";
}
<div id="carousel" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
        <li data-target="#carousel" data-slide-to="0" class="active"></li>       
        <li data-target="#carousel" data-slide-to="1"></li>
    </ol>
    <div class="carousel-inner">
        <div class="item active">
            <img src="~/images/Photos/Sword.png" style="width:100%; height:400px">
        </div>        
        <div class="item">
            <img src="~/images/Photos/Tree.png" style="width:100%; height:400px">
        </div>
    </div>
</div>
